<MudGrid>
    <MudItem xs="11">
        <MudText Class="site-logo-text site-title-large" Typo="Typo.overline" Align="Align.Center" Color="Color.Primary">@Title</MudText>
    </MudItem>
    @ChildContent
</MudGrid>

@code {
    [Parameter]
    public String Title { get; set; }
    [Parameter]
    public RenderFragment ChildContent { get; set; }
}
